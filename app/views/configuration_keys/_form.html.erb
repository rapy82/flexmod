<%= simple_form_for(@configuration_key) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :key %>
    <%= f.input :value %>
    <%= f.input :description %>
    <%= f.input :section %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
